    "class Counter:\n",
    "    count = 0  # Static variable\n",
    "\n",
    "    def __init__(self):\n",
    "        Counter.count += 1\n",
    "    \n",
    "    @staticmethod\n",
    "    def show_count():\n",
    "        print(f\"Count is: {Counter.count}\")\n",
    "\n",
    "# Usage\n",
    "a = Counter()\n",
    "b = Counter()\n",
    "c = Counter()\n",
    "\n",
    "Counter.show_count()  # Output: Count is: 3\n"
   ]
